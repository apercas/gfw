/** category visualization */

#peru_forest_concessions {
   polygon-opacity: 0.7;
   line-color: #FFF;
   line-width: 1;
   line-opacity: 1;
}

#peru_forest_concessions[supervision="SUPERVISED BY OSINFOR"] {
   polygon-fill: #1F78B4;
}
#peru_forest_concessions[supervision="UNSUPERVISED"] {
   polygon-fill: #A6CEE3;
}
